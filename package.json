{
  "name": "tennis",
  "version": "1.0",
  "description": "https://tenisu.latelier.co/backend",
  "author": "viiinzzz",
  "private": true,
  "license": "MIT",
  "scripts": {
    "upgrade": "yarn upgrade-interactive --latest",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:cloud": "sls offline start",
    "start:cloud": "cross-env AWS_CLIENT_TIMEOUT=300000 serverless deploy --org=viiinzzz --aws-s3-accelerate",
    "doc__________LIVINGDOC_SCRIPTS__________": "",
    "doc:cloud": "sls graph -o ./docs/cloud.out&& cross-cat ./docs/cloud.out | dot -Tpng > ./docs/cloud.png&&\"%LOCALAPPDATA%\\Programs\\Microsoft VS Code\\Code.exe\" ./docs/cloud.png",
    "doc:cloud:noregen": "cross-cat ./docs/cloud.out | dot -Tpng > ./docs/cloud.png&&\"%LOCALAPPDATA%\\Programs\\Microsoft VS Code\\Code.exe\" ./docs/cloud.png",
    "doc:dependencies": "yarn --silent depcruise src --include-only \"^src\" --config --output-type dot | dot -Tpng > ./docs/dependencies.png&&\"%LOCALAPPDATA%\\Programs\\Microsoft VS Code\\Code.exe\" ./docs/dependencies.png",
    "·☁️·doc:cloud": "yarn doc:cloud",
    "·🧲·doc:dependencies": "yarn doc:dependencies"
  },
  "dependencies": {
    "@nestjs/common": "^9.2.1",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.2.1",
    "@nestjs/mongoose": "^9.2.1",
    "@nestjs/platform-express": "^9.2.1",
    "@nestjs/swagger": "^6.1.4",
    "@vendia/serverless-express": "^4.10.1",
    "aws-lambda": "^1.0.7",
    "aws-serverless-express": "^3.4.0",
    "mongoose": "^6.8.4",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^4.1.1",
    "rxjs": "^7.8.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.1.8",
    "@nestjs/schematics": "^9.0.4",
    "@nestjs/testing": "^9.2.1",
    "@types/express": "^4.17.11",
    "@types/jest": "^29.2.6",
    "@types/node": "^18.11.18",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "@typescript-eslint/parser": "^5.48.2",
    "cross-cat": "^1.0.0",
    "cross-env": "^7.0.3",
    "dependency-cruiser": "^12.5.0",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.3.1",
    "nestjs-bdd": "^0.4.2",
    "plugin": "^0.3.3",
    "prettier": "^2.2.1",
    "serverless-graph": "^0.3.2",
    "serverless-jetpack": "^0.11.1",
    "serverless-offline": "^12.0.4",
    "serverless-plugin-optimize": "^4.2.1-rc.1",
    "serverless-plugin-typescript": "^2.1.4",
    "supertest": "^6.1.3",
    "ts-jest": "^29.0.5",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^4.1.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
